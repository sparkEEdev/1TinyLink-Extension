{
	"manifest_version": 2,
	"name": "1TinyLink",
	"description": "",
	"version": "0.8",
	"author": "",
	"permissions": ["activeTab", "storage", "tabs", "contextMenus"],
	"browser_action": {
		"default_icon": "images/icon120.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon32.png",
		"128": "images/icon120.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"css": [
				"resources/css/style.css"
			]
		}
	],
	"web_accessible_resources": [
		"content/formIFrame.html",
		"content/responseIFrame.html",
		"resources/css/frame.css"
	]
}
